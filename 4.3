#include <iostream>
#include <iomanip> 

int main() {
    const int rows = 10;
    const int cols = 10;
    int tab[rows][cols] = {0}; 

    for (int i = 0; i < rows; i++) {
        tab[i][0] = i;           
        tab[i][1] = i + i;       
        tab[i][2] = i * i;       
        for (int j = 3; j < cols; j++) {
            if (j == 3)
                tab[i][j] = i + j;   
            else if (j == 4)
                tab[i][j] = i - j;   
            else
                tab[i][j] = 0;       
        }
    }

    std::cout << "Tablica 10x10:\n";
    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < cols; j++) {
            std::cout << std::setw(5) << tab[i][j]; 
        }
        std::cout << std::endl;
    }

    return 0;
}
