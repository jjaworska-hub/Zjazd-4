#include <iostream>

int main() {
    int n;

    std::cout << "Podaj liczbe elementow tablicy: ";
    std::cin >> n;

    if (n <= 0) {
        std::cout << "Liczba elementow musi byc dodatnia." << std::endl;
    }

    int tabela[n];

    std::cout << "Podaj " << n << " liczb calkowitych:" << std::endl;
    for (int i = 0; i < n; i++) {
        std::cin >> tabela[i]; 
    }

    int max = tabela[0]; 

    for (int i = 1; i < n; i++) {
        if (tabela[i] > max) {
            max = tabela[i]; 
        }
    }

    std::cout << "Najwiekszy element tablicy to: " << max << std::endl;

    return 0;
}
